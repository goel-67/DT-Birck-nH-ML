# Development Environment Configuration Template
# Copy this to development.env and fill in actual values

# Data Sources
AZURE_DATASET_URL=
AZURE_LAST_PROCESSED_TIMESTAMP_URL=
SHAREPOINT_CREDENTIALS_ENABLED=true

# Database Configuration
SQLITE_DB_PATH=pareto_cache.db
CACHE_DIRECTORY=dt-cache
DATABASE_BACKUP_ENABLED=false

# Processing Configuration
MAX_ITERATIONS=4
POINTS_PER_ITERATION=3
MODEL_CONFIGURATION_VERSION=v1.3.0

# Training Data Cutoff Method
# Options: "first_recipe_current" or "last_recipe_previous"
DT_TRAINING_CUTOFF_METHOD=first_recipe_current

# GPR Hyperparameter Optimization
# Set to "true" to enable hyperparameter optimization for GPR models (slower but potentially better performance)
# Set to "false" to use default GPR parameters (faster training) - DEFAULT
DT_GPR_HYPEROPT=false

# Cache Management Mode
# Set to "full_rebuild" to clear all cache and rebuild from scratch - DEFAULT
# Set to "incremental" to build upon existing cache (faster startup)
CACHE_MANAGEMENT_MODE=incremental
# Cache Validation
# Set to "false" to disable cache validation (allows incremental mode to work)
CACHE_VALIDATE_ON_START=false

# Cache Plot Regeneration
# Set to "true" to generate plots during cache regeneration (slower but complete)
# Set to "false" to skip plots during cache regeneration (faster startup)
CACHE_REGENERATE_PLOTS=true

# nanoHUB Integration
NANOHUB_API_ENDPOINT=
NANOHUB_CREDENTIALS_ENABLED=false

# Logging
LOG_LEVEL=DEBUG
LOG_FILE=pareto_optimization_dev.log

# Performance
ENABLE_PARALLEL_PROCESSING=false
MAX_WORKERS=1
CACHE_VALIDATION_ENABLED=true

# Azure Integration (for future use)
AZURE_STORAGE_CONNECTION_STRING=
AZURE_CONTAINER_NAME=
AZURE_BLOB_PREFIX=
