# Pareto Front Improvement Thresholds
# These control how much a point must improve to be considered "significantly improving"

# Rate improvement threshold (as fraction of current best rate)
# Higher values = more aggressive filtering, only points with larger improvements
DT_RATE_THRESHOLD=0.05

# Range improvement threshold (as fraction of current best range)  
# Higher values = more aggressive filtering, only points with larger improvements
DT_RANGE_THRESHOLD=0.05

# Example: Setting both to 0.10 means a point must improve by at least 10%
# in either rate or range to be considered significantly improving

# Original optimization weights (unchanged)
DT_ALPHA=1.0
DT_BETA=0.25
DT_GAMMA=0.25

# ==================== DATASET & CACHE CONFIG ====================
DT_DATASET_CSV=data/full_dataset.csv
DT_CACHE_ROOT=dt-cache

# ==================== SAMPLING CONFIG ====================
DT_TOP_K=3
DT_SAMPLING=sobol
DT_SEED=42
DT_SOBOL_POW2=true
DT_NSAMPLES=200000

# ==================== TARGET RATE CONSTRAINTS ====================
DT_RATE_MIN=35
DT_RATE_MAX=110

# ==================== ITERATION CONFIG ====================
DT_POINTS_PER_ITERATION=3
DT_INGEST_MODE=auto

# ==================== TRAINING DATA CUTOFF CONFIG ====================
# Method for determining training data cutoff date for each iteration
# Options: "first_recipe_current" or "last_recipe_previous"
# - first_recipe_current: Use completion date of first recipe in current iteration (original approach)
# - last_recipe_previous: Use completion date of last recipe in previous iteration (alternative approach)
DT_TRAINING_CUTOFF_METHOD=first_recipe_current

# ==================== MODEL CONFIG ====================
DT_MODEL_CHOICE=rf_both

# ==================== LOCAL EXCEL CONFIG (BYPASSES SHAREPOINT) ====================
DT_RECIPES_XLSX=data/test_pareto_recipes.xlsx

# ==================== EXCEL COLUMN MAPPINGS ====================
DT_EXCEL_STATUS_COL=Status
DT_EXCEL_DATE_COL=Date_Completed
DT_EXCEL_LOT_COL=Lotname
DT_EXCEL_INGEST_COL=Ingestion_status
DT_EXCEL_PRED_RATE_COL=Pred_avg_etch_rate
DT_EXCEL_PRED_RANGE_COL=Pred_Range
DT_EXCEL_RATE_UNCERTAINTY_COL=Etch_rate_uncertainty
DT_EXCEL_RANGE_UNCERTAINTY_COL=Range_uncertainty

